{
  "name": "p5ble",
  "version": "0.0.8",
  "description": "A javascript library that enables communication between BLE devices and a p5 sketch.",
  "main": "dist/p5.ble.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "start": "webpack serve --mode development --config webpack.dev.js",
    "build": "npm-run-all --parallel build:*",
    "build:umd": "webpack --config webpack.config.js --output-filename p5.ble.js --mode development",
    "build:umd.min": "webpack --config webpack.config.js --output-filename p5.ble.min.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ITPNYU/p5.ble.js.git"
  },
  "keywords": [
    "p5js",
    "ble"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Yining Shi <yining@nyu.edu> (https://1023.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ITPNYU/p5.ble.js/issues"
  },
  "homepage": "https://github.com/ITPNYU/p5.ble.js#readme",
  "devDependencies": {
    "all-contributors-cli": "^6.26.1",
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.22.5",
    "babel-loader": "10.0.0",
    "eslint": "9.39.2",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "5.6.6",
    "npm-run-all": "4.1.5",
    "rimraf": "6.1.2",
    "webpack": "5.104.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.3",
    "webpack-merge": "6.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "idempotent-babel-polyfill": "7.4.4"
  }
}
